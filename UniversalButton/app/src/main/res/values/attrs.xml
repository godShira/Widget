<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="universalBtn">
        <attr name="normalSolid" format="color" />
        <attr name="pressedSolid" format="color" />
        <attr name="stroke" format="color" />
        <attr name="normalStroke" format="color" />
        <attr name="pressedStroke" format="color" />
        <attr name="roundButtonRadius" format="dimension" />
        <attr name="roundButtonLeftTopRadius" format="dimension" />
        <attr name="roundButtonLeftBottomRadius" format="dimension" />
        <attr name="roundButtonRightTopRadius" format="dimension" />
        <attr name="roundButtonRightBottomRadius" format="dimension" />
        <attr name="normalDrawable" format="reference" />
        <attr name="pressedDrawable" format="reference" />
        <attr name="isSelected" format="boolean" />
        <attr name="normalTextColor" format="color" />
        <attr name="selectedTextColor" format="color" />
        <attr name="strokeWidth" format="dimension" />
    </declare-styleable>

</resources>
